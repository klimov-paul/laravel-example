{
    "name": "klimov-paul/laravel-example",
    "type": "project",
    "description": "The Laravel Project Example.",
    "keywords": [
        "laravel",
        "example",
        "training",
        "education"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "bensampo/laravel-enum": "^5.3",
        "braintree/braintree_php": "^6.5.1",
        "guzzlehttp/guzzle": "^7.2",
        "illuminatech/data-provider": "^1.0.3",
        "illuminatech/model-rules": "^1.0.0",
        "laravel/framework": "^9.19",
        "laravel/horizon": "^5.9.7",
        "laravel/sanctum": "^2.14.1",
        "laravel/telescope": "^4.9.0",
        "laravel/tinker": "^2.7",
        "mad-web/laravel-initializer": "^3.4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.8.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"file_exists('bootstrap/cache/config.php') && unlink('bootstrap/cache/config.php');\"",
            "@php -r \"file_exists('bootstrap/cache/services.php') && unlink('bootstrap/cache/services.php');\"",
            "@php -r \"file_exists('bootstrap/cache/packages.php') && unlink('bootstrap/cache/packages.php');\"",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "project-install": [
            "@composer install --no-dev",
            "@php artisan app:install"
        ],
        "project-update": [
            "@composer install --no-dev",
            "@php artisan app:update"
        ],
        "fix-cs": [
            "php-cs-fixer --verbose fix"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
